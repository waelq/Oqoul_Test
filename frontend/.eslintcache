eToast from \"./Toaster\";\r\n\r\nconst App = () => {\r\n  const [socket, setSocket] = React.useState(null);\r\n\r\n  const setupSocket = () => {\r\n    const token = localStorage.getItem(\"CC_Token\");\r\n    if (token && !socket) {\r\n      const newSocket = io(\"http://localhost:8000\", {\r\n        query: {\r\n          token: localStorage.getItem(\"CC_Token\"),\r\n        },\r\n      });\r\n\r\n      newSocket.on(\"disconnect\", () => {\r\n        setSocket(null);\r\n        setTimeout(setupSocket, 3000);\r\n        makeToast(\"error\", \"Socket Disconnected!\");\r\n      });\r\n\r\n      newSocket.on(\"connect\", () => {\r\n        makeToast(\"success\", \"Socket Connected!\");\r\n      });\r\n\r\n      setSocket(newSocket);\r\n    }\r\n  };\r\n\r\n  React.useEffect(() => {\r\n    setupSocket();\r\n    //eslint-disable-next-line\r\n  }, []);\r\n  return (\r\n    <Router>\r\n      <Header />\r\n      <main className=\"py-3\">\r\n        <Container>\r\n          <Route path=\"/login\" component={LoginScreen} />\r\n          <Route path=\"/register\" component={RegisterScreen} />\r\n          <Route path=\"/profile\" component={ProfileScreen} />\r\n          <Route path=\"/post/:id\" component={PostScreen} />\r\n          <Route path=\"/admin/userlist\" component={UserListScreen} />\r\n          <Route path=\"/admin/user/:id/edit\" component={UserEditScreen} />\r\n\r\n          <Route path=\"/admin/postlist\" component={PostsListScreen} />\r\n          <Route path=\"/user/post/:id/edit\" component={PostEditScreen} />\r\n          <Route path=\"/posts/user/:id\" component={PostOneUser} />\r\n\r\n          <Route path=\"/\" component={HomeScreen} exact />\r\n          <Route path=\"/search/:keyword\" component={HomeScreen} exact />\r\n\r\n          <Route\r\n            path=\"/dashboard\"\r\n            render={() => <DashboardPage socket={socket} />}\r\n            exact\r\n          />\r\n          <Route\r\n            path=\"/chatroom/:id\"\r\n            render={() => <ChatroomPage socket={socket} />}\r\n            exact\r\n          />\r\n        </Container>\r\n      </main>\r\n      <Footer />\r\n    </Router>\r\n  );\r\n};\r\n\r\nexport default App;\r\n","C:\\Users\\WaelTq\\Desktop\\Oqoul_Test\\frontend\\src\\reportWebVitals.js",[],"C:\\Users\\WaelTq\\Desktop\\Oqoul_Test\\frontend\\src\\Components\\Footer.js",[],"C:\\Users\\WaelTq\\Desktop\\Oqoul_Test\\frontend\\src\\Components\\Header.js",[],"C:\\Users\\WaelTq\\Desktop\\Oqoul_Test\\frontend\\src\\Screens\\HomeScreen.js",[],"C:\\Users\\WaelTq\\Desktop\\Oqoul_Test\\frontend\\src\\post.js",[],["129","130"],"C:\\Users\\WaelTq\\Desktop\\Oqoul_Test\\frontend\\src\\Components\\Post.js",[],"C:\\Users\\WaelTq\\Desktop\\Oqoul_Test\\frontend\\src\\Screens\\PostScreen.js",[],"C:\\Users\\WaelTq\\Desktop\\Oqoul_Test\\frontend\\src\\store.js",[],"C:\\Users\\WaelTq\\Desktop\\Oqoul_Test\\frontend\\src\\reducer\\postReducer.js",[],"C:\\Users\\WaelTq\\Desktop\\Oqoul_Test\\frontend\\src\\constants\\postConstants.js",[],"C:\\Users\\WaelTq\\Desktop\\Oqoul_Test\\frontend\\src\\action\\postAction.js",[],"C:\\Users\\WaelTq\\Desktop\\Oqoul_Test\\frontend\\src\\Components\\Message.js",[],"C:\\Users\\WaelTq\\Desktop\\Oqoul_Test\\frontend\\src\\Components\\Loader.js",[],"C:\\Users\\WaelTq\\Desktop\\Oqoul_Test\\frontend\\src\\reducer\\userReducer.js",[],"C:\\Users\\WaelTq\\Desktop\\Oqoul_Test\\frontend\\src\\constants\\userConstants.js",[],"C:\\Users\\WaelTq\\Desktop\\Oqoul_Test\\frontend\\src\\Screens\\LoginScreen.js",[],"C:\\Users\\WaelTq\\Desktop\\Oqoul_Test\\frontend\\src\\action\\userAction.js",[],"C:\\Users\\WaelTq\\Desktop\\Oqoul_Test\\frontend\\src\\Screens\\RegisterScreen.js",[],"C:\\Users\\WaelTq\\Desktop\\Oqoul_Test\\frontend\\src\\Screens\\ProfileScreen.js",[],"C:\\Users\\WaelTq\\Desktop\\Oqoul_Test\\frontend\\src\\Screens\\UserListScreen.js",[],"C:\\Users\\WaelTq\\Desktop\\Oqoul_Test\\frontend\\src\\Screens\\PostsListScreen.js",[],"C:\\Users\\WaelTq\\Desktop\\Oqoul_Test\\frontend\